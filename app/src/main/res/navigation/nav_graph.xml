<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/status">
    <fragment
        android:id="@+id/status"
        android:name="de.lorenzgorse.coopmobile.fragments.StatusFragment"
        tools:layout="@layout/fragment_status">
        <action
            android:id="@+id/action_status_to_login2"
            app:destination="@id/login"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/status"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_status_to_add_product3"
            app:destination="@id/add_product"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_status_to_correspondences"
            app:destination="@id/correspondences"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_status_to_web_view"
            app:destination="@id/web_view"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/login"
        android:name="de.lorenzgorse.coopmobile.fragments.LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_login_to_status2"
            app:destination="@id/status"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/status"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/add_product"
        android:name="de.lorenzgorse.coopmobile.fragments.AddProductFragment"
        tools:layout="@layout/fragment_add_product">
        <action
            android:id="@+id/action_add_product_to_login2"
            app:destination="@id/login"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/status"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_add_product_to_buy_product"
            app:destination="@id/buy_product"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@+id/add_product"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/correspondences"
        android:name="de.lorenzgorse.coopmobile.fragments.CorrespondencesFragment"
        tools:layout="@layout/fragment_correspondences">
        <action
            android:id="@+id/action_correspondences_to_login"
            app:destination="@id/login"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/status"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/buy_product"
        android:name="de.lorenzgorse.coopmobile.fragments.BuyProductFragment" >
        <argument
            android:name="product"
            app:argType="de.lorenzgorse.coopmobile.ProductBuySpec" />
    </fragment>
    <fragment
        android:id="@+id/web_view"
        android:name="de.lorenzgorse.coopmobile.fragments.WebViewFragment"
        tools:layout="@layout/fragment_web_view" >
        <action
            android:id="@+id/action_web_view_to_login"
            app:destination="@id/login"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/status"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>
